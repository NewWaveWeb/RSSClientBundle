<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="d2_client_rss.class">Desarrolla2\Bundle\RSSClientBundle\Service\RSSClient</parameter>
    </parameters>

    <services>
        <service id="d2_client_rss" class="%d2_client_rss.class%">
            <call method="setChannels">
                <argument>%d2_client_rss.channels%</argument>
            </call>
        </service>
    </services>
</container>